module generator

amends "pkl:test"
import "../gen.pkl" as t


local class Foo {
    x: Int
}


local dynamic: Dynamic = new Dynamic {
   x = 1
}

examples {
    ["Basic Class"] {
        t.apply(Foo, dynamic).toDynamic()
    }
}

facts {
    ["Basic Class"]  {
        new Foo { x = 1 } == new Foo { x = 1 }
    }
}