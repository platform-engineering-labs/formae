{
  "Targets": [
    {
      "Label": "us-west-1",
      "Namespace": "AWS",
      "Config": {
        "Region": "us-west-1",
        "Type": "AWS"
      }
    }
  ],
  "Resources": [
    {
      "Label": "example-lambda-function",
      "Type": "AWS::Lambda::Function",
      "Stack": "$unmanaged",
      "Target": "us-west-1",
      "Properties": {
        "Architectures": [
          "x86_64"
        ],
        "Arn": "arn:aws:lambda:us-west-1:123456789012:function:example-lambda-function",
        "Description": "",
        "Environment": {
          "Variables": {
            "SES_SENDER_EMAIL": "example@example.com"
          }
        },
        "EphemeralStorage": {
          "Size": 512
        },
        "FunctionName": "example-lambda-function",
        "Handler": "handler.exampleFunction",
        "LoggingConfig": {
          "LogFormat": "Text",
          "LogGroup": "/aws/lambda/example-lambda-function"
        },
        "MemorySize": 1024,
        "PackageType": "Zip",
        "Role": "arn:aws:iam::123456789012:role/example-lambda-role",
        "Runtime": "nodejs18.x",
        "SnapStartResponse": {
          "ApplyOn": "None",
          "OptimizationStatus": "Off"
        },
        "Timeout": 6,
        "TracingConfig": {
          "Mode": "PassThrough"
        },
        "VpcConfig": {
          "Ipv6AllowedForDualStack": false
        }
      },
      "NativeId": "example-lambda-function"
    }
  ]
}