{
  "Targets": [
    {
      "Label": "us-west-1",
      "Namespace": "AWS",
      "Config": {
        "Region": "us-west-1",
        "Type": "AWS"
      }
    }
  ],
  "Resources": [
    {
      "Label": "example-bucket-policy-1",
      "Type": "AWS::S3::BucketPolicy",
      "Stack": "$unmanaged",
      "Target": "us-west-1",
      "Properties": {
        "Bucket": "example-bucket-policy-1",
        "PolicyDocument": "{\"Version\":\"2012-10-17\",\"Id\":\"AccessControl\",\"Statement\":[{\"Sid\":\"AllowSSLRequestsOnly\",\"Effect\":\"Deny\",\"Principal\":\"*\",\"Action\":\"s3:*\",\"Resource\":[\"arn:aws:s3:::example-bucket-policy-1\",\"arn:aws:s3:::example-bucket-policy-1/*\"],\"Condition\":{\"Bool\":{\"aws:SecureTransport\":\"false\"}}}]}"
      },
      "ReadOnlyProperties": {
        "Bucket": "example-bucket-policy-1",
        "PolicyDocument": {
          "Id": "AccessControl",
          "Statement": [
            {
              "Action": "s3:*",
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": "false"
                }
              },
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                "arn:aws:s3:::example-bucket-policy-1",
                "arn:aws:s3:::example-bucket-policy-1/*"
              ],
              "Sid": "AllowSSLRequestsOnly"
            }
          ],
          "Version": "2012-10-17"
        }
      },
      "NativeId": "example-bucket-policy-1"
    },
    {
      "Label": "example-bucket-policy-2",
      "Type": "AWS::S3::BucketPolicy",
      "Stack": "$unmanaged",
      "Target": "us-west-1",
      "Properties": {
        "Bucket": "example-bucket-policy-2",
        "PolicyDocument": "{\"Version\":\"2008-10-17\",\"Statement\":[{\"Effect\":\"Deny\",\"Principal\":\"*\",\"Action\":\"s3:*\",\"Resource\":[\"arn:aws:s3:::example-bucket-policy-2/*\",\"arn:aws:s3:::example-bucket-policy-2\"],\"Condition\":{\"Bool\":{\"aws:SecureTransport\":\"false\"}}}]}"
      },
      "ReadOnlyProperties": {
        "Bucket": "example-bucket-policy-2",
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": "false"
                }
              },
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                "arn:aws:s3:::example-bucket-policy-2/*",
                "arn:aws:s3:::example-bucket-policy-2"
              ]
            }
          ],
          "Version": "2008-10-17"
        }
      },
      "NativeId": "example-bucket-policy-2"
    }
  ]
}