amends "@formae/forma.pkl"
import "@formae/formae.pkl"

import "@aws/aws.pkl"

import "@aws/s3/bucket.pkl"

local b = new bucket.Bucket {
  label = "plugin-sdk-test-bucket"
  bucketName = "formae-plugin-sdk-test-s3-20250103"
}

local stackName = "plugin-sdk-test-stack"

forma {
  new formae.Stack {
    label = stackName
    description = "Plugin SDK test for S3 Bucket"
  }

  new formae.Target {
    label = "aws-target"
    config = new aws.Config {
      region = "us-east-1"
    }
  }

  b
}
