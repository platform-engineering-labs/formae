module aws.iam.managedpolicy

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::IAM::ManagedPolicy"

@aws.ResourceHint {
    type = module.type
    identifier = "PolicyArn"
    discoverable = false
    extractable = true
}
open class ManagedPolicy extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    description: String?

    @aws.FieldHint
    groups: Listing<String>?

    @aws.FieldHint{createOnly = true}
    managedPolicyName: String?

    @aws.FieldHint{createOnly = true}
    path: String?

    @aws.FieldHint
    policyDocument: Dynamic

    @aws.FieldHint
    roles: Listing<String|formae.Resolvable>?

    @aws.FieldHint
    users: Listing<String|formae.Resolvable>?
}
