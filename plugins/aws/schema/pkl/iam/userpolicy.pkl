module aws.iam.userpolicy

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::IAM::UserPolicy"

@aws.ResourceHint {
    type = module.type
    identifier = "PolicyName"
    discoverable = false
}
open class UserPolicy extends formae.Resource {

    @aws.FieldHint
    policyDocument: Dynamic?

    @aws.FieldHint{createOnly = true}
    policyName: String

    @aws.FieldHint{createOnly = true}
    userName: String
}
