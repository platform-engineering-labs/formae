module aws.sqs.queuepolicy

import "../aws.pkl"
import "@formae/formae.pkl"

const type = "AWS::SQS::QueuePolicy"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
}
open class QueuePolicy extends formae.Resource {

    @aws.FieldHint
    policyDocument: Dynamic

    @aws.FieldHint
    queues: Listing<String|formae.Resolvable>
}
