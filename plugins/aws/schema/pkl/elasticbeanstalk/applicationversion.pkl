module aws.elasticbeanstalk.applicationversion

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::ElasticBeanstalk::ApplicationVersion"

@aws.SubResourceHint
open class ApplicationVersionSourceBundle extends formae.SubResource {
    s3Bucket: String
    s3Key: String
}

@aws.ResourceHint {
    type = module.type
    identifier = "ApplicationName"
}
open class ApplicationVersion extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    applicationName: String|formae.Resolvable

    @aws.FieldHint
    description: String?

    @aws.FieldHint{createOnly = true}
    sourceBundle: ApplicationVersionSourceBundle
}
