module aws.lambda.layerversionpermission

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::Lambda::LayerVersionPermission"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    parent = "AWS::Lambda::LayerVersion"
    listParam = new formae.ListProperty { parentProperty = "LayerVersionArn" listParameter = "LayerVersionArn" }
    discoverable = false
}
open class LayerVersionPermission extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    action: String

    @aws.FieldHint{createOnly = true}
    layerVersionArn: String|formae.Resolvable

    @aws.FieldHint{createOnly = true}
    organizationId: String?

    @aws.FieldHint{createOnly = true}
    principal: String
}
