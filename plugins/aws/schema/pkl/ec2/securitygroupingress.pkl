module aws.ec2.securitygroupingress

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::SecurityGroupIngress"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class SecurityGroupIngress extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    cidrIp: String?

    @aws.FieldHint{createOnly = true}
    cidrIpv6: String?

    @aws.FieldHint
    description: String?

    @aws.FieldHint{createOnly = true}
    fromPort: Int?

    @aws.FieldHint{createOnly = true}
    groupId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    groupName: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    ipProtocol: String

    @aws.FieldHint{createOnly = true}
    sourcePrefixListId: String?

    @aws.FieldHint{createOnly = true}
    sourceSecurityGroupId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    sourceSecurityGroupName: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    sourceSecurityGroupOwnerId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    toPort: Int?
}
