module aws.ec2.trafficmirrorsession

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::TrafficMirrorSession"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    tags = "Tags"
}
open class TrafficMirrorSession extends formae.Resource {

    @aws.FieldHint
    description: String?

    @aws.FieldHint{createOnly = true}
    networkInterfaceId: String|formae.Resolvable

    @aws.FieldHint
    packetLength: Int?

    @aws.FieldHint
    sessionNumber: Int

    @aws.FieldHint
    tags: Listing<formae.Tag>?

    @aws.FieldHint
    trafficMirrorFilterId: String|formae.Resolvable

    @aws.FieldHint
    trafficMirrorTargetId: String|formae.Resolvable

    @aws.FieldHint
    virtualNetworkId: (Int|formae.Resolvable)?
}
