module aws.ec2.networkinterfacepermission

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::NetworkInterfacePermission"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
}
open class NetworkInterfacePermission extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    awsAccountId: String

    @aws.FieldHint{createOnly = true}
    networkInterfaceId: String|formae.Resolvable

    @aws.FieldHint{createOnly = true}
    permission: String
}
