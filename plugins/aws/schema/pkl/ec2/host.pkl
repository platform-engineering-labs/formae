module aws.ec2.host

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::Host"


@aws.ResourceHint {
    type = module.type
    identifier = "HostId"
}
open class Host extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    assetId: String?

    @aws.FieldHint
    autoPlacement: String?

    @aws.FieldHint{createOnly = true}
    availabilityZone: aws.AvailabilityZone

    @aws.FieldHint
    hostMaintenance: String?

    @aws.FieldHint
    hostRecovery: String?

    @aws.FieldHint{createOnly = true}
    instanceFamily: String?

    @aws.FieldHint{createOnly = true}
    instanceType: String?

    @aws.FieldHint{createOnly = true}
    outpostArn: String?
}
