module aws.ec2.trafficmirrortarget

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::TrafficMirrorTarget"


@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    tags = "Tags"
}
open class TrafficMirrorTarget extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    description: String?

    @aws.FieldHint{createOnly = true}
    gatewayLoadBalancerEndpointId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    networkInterfaceId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    networkLoadBalancerArn: (String|formae.Resolvable)?

    @aws.FieldHint
    tags: Listing<formae.Tag>?
}
