module aws.ec2.enclavecertificateiamroleassociation

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::EnclaveCertificateIamRoleAssociation"


@aws.ResourceHint {
    type = module.type
    identifier = "CertificateArn"
    discoverable = false
}
open class EnclaveCertificateIamRoleAssociation extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    certificateArn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:acm:[A-Za-z0-9-]{1,64}:([0-9]{12})?:certificate/.+$"#)))|formae.Resolvable

    @aws.FieldHint{createOnly = true}
    roleArn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:iam:.*:([0-9]{12})?:role/.+$"#)))|formae.Resolvable
}
