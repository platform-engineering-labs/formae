module aws.ec2.ipamscope

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::IPAMScope"

@aws.ResourceHint {
    type = module.type
    identifier = "IpamScopeId"
    tags = "Tags"
}
open class IPAMScope extends formae.Resource {

    @aws.FieldHint
    description: String?

    @aws.FieldHint{createOnly = true}
    ipamId: String|formae.Resolvable

    @aws.FieldHint
    tags: Listing<formae.Tag>?
}
