module aws.ec2.networkaclentry

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::NetworkAclEntry"

@aws.SubResourceHint
open class Icmp extends formae.SubResource {
    code: Int?
    type: Int?
}

@aws.SubResourceHint
open class PortRange extends formae.SubResource {
    from: Int?
    to: Int?
}

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
}
open class NetworkAclEntry extends formae.Resource {

    @aws.FieldHint
    cidrBlock: String?

    @aws.FieldHint{createOnly = true}
    egress: Boolean?

    @aws.FieldHint
    icmp: Icmp?

    @aws.FieldHint
    ipv6CidrBlock: String?

    @aws.FieldHint{createOnly = true}
    networkAclId: String|formae.Resolvable

    @aws.FieldHint
    portRange: PortRange?

    @aws.FieldHint
    protocol: Int

    @aws.FieldHint
    ruleAction: String

    @aws.FieldHint{createOnly = true}
    ruleNumber: Int
}
