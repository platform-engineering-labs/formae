module aws.ec2.eipassociation

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::EIPAssociation"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class EIPAssociation extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    allocationId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    eIP: String?

    @aws.FieldHint{createOnly = true}
    instanceId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    networkInterfaceId: (String|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    privateIpAddress: String?
}
