module aws.ec2.subnetcidrblock

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::SubnetCidrBlock"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class SubnetCidrBlock extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    ipv6CidrBlock: String?

    @aws.FieldHint{
        createOnly = true
        writeOnly = true
    }
    ipv6IpamPoolId: String?

    @aws.FieldHint{
        createOnly = true
        writeOnly = true
    }
    ipv6NetmaskLength: Int?

    @aws.FieldHint{createOnly = true}
    subnetId: String|formae.Resolvable
}
