module aws.secretsmanager.secrettargetattachment

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::SecretsManager::SecretTargetAttachment"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
    extractable = true
}
open class SecretTargetAttachment extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    secretId: String|formae.Resolvable

    @aws.FieldHint
    targetId: String|formae.Resolvable

    @aws.FieldHint
    targetType: String
}
