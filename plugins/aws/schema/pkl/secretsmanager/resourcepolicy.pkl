module aws.secretsmanager.resourcepolicy

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::SecretsManager::ResourcePolicy"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class ResourcePolicy extends formae.Resource {

    @aws.FieldHint{writeOnly = true}
    blockPublicPolicy: Boolean?

    @aws.FieldHint
    resourcePolicy: Dynamic

    @aws.FieldHint{createOnly = true}
    secretId: String
}
