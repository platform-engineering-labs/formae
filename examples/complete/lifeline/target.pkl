amends "@formae/forma.pkl"
import "@formae/formae.pkl"
import "@aws/aws.pkl"

description {
    text = """
    This is the target for the lifeline showcase.
    """
    confirm = true
}

forma {
    new formae.Target {
        label = "aws-target-west-1-disco"
